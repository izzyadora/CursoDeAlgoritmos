algoritmo "Exercício6"
//Faça um programa que leia um número inteiro e mostre o seu antecessor e seu sucessor.
var
   a,s,n:inteiro
inicio
      escreval("Digite um número: ")
      leia(n)
      s <- n+1
      a <- n-1
      escreval("O antecessor de",n," é",a)
      escreval("O sucesssor de",n," é",s)
fimalgoritmo