algoritmo "semnome"
var
   n:inteiro
inicio
      escreva("Digite um valor para n: ")
      leia(n)
      enquanto (n > 10) faca
          escreva("Digite novamente: ")
          leia(n)
      fimenquanto
      
      repita
            escreva("Digite um valor para n: ")
            leia(n)
      ate (n <= 10)
fimalgoritmo